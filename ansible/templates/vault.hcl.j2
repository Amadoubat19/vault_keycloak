disable_cache = true
disable_mlock = true
ui = true
listener "tcp" {
   address          = "127.0.0.1:8200"
   tls_disable      = 1
}

storage "raft" {
   path  = "/var/lib/vault/data"
}

api_addr        = "http://127.0.0.1:8200"
cluster_addr    = "http://127.0.0.1:8201"
max_lease_ttl   = "10h"
default_lease_ttl       = "10h"
cluster_name    = "vault"